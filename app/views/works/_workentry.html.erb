<div>
	<%= content_tag :h5, work.name %>
	<%= content_tag :h4, work.series if work.series %>
  	<%= content_tag :h4, work.publisher if work.publisher %>
    <%= work.embed.html_safe %>
	<%= content_tag :h3, work.av %>
	<%= content_tag :h4, work.category %>
	<%= @work.role %>
<p>
	<%= link_to 'Show', work %>
    <%= link_to 'Edit', edit_work_path(work) %>
    <%= link_to 'Destroy', work, method: :delete, data: { confirm: 'Are you sure?' } %>
</p>
</div>